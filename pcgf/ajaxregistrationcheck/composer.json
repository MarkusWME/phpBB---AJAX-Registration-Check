{
    "name": "pcgf/ajaxregistrationcheck",
    "type": "phpbb-extension",
    "description": "An extension that checks data on the registration form with AJAX so that no invalid or unwanted data could be typed in",
    "homepage": "https://github.com/MarkusWME/phpBB---AJAX-Registration-Check",
    "version": "1.0.0",
    "time": "2017-05-20",
    "keywords": ["ajax", "registration", "check", "registration check", "ajax check", "ajax registration check"],
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "MarkusWME",
            "homepage": "https://github.com/MarkusWME",
            "email": "markuswme@pcgamingfreaks.at",
            "role": "Lead Developer"
        }
    ],
    "require": {
        "php": ">=5.3.3"
    },
    "extra": {
        "display-name": "AJAX Registration Check",
        "soft-require": {
            "phpbb/phpbb": ">=3.1"
        },
        "version-check": {
            "host": "api.pcgf.at",
            "directory": "/phpbb",
            "filename": "ajaxregistrationcheck.json"
        }
    }
}